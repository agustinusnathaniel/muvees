{
  "name": "muvees",
  "version": "1.3.4",
  "private": true,
  "author": "agustinusnathaniel",
  "engines": {
    "node": ">=22.16.0",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "lint": "next lint",
    "turbo": "pnpm dlx turbo@2.3.3",
    "lint:fix": "eslint src --fix && pnpm format",
    "build": "next build",
    "build:turbo": "pnpm turbo build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "check:turbo": "pnpm turbo lint type-check",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/outfit": "^5.2.5",
    "formik": "^2.4.6",
    "framer-motion": "^11.13.0",
    "ky": "^1.8.1",
    "lodash": "^4.17.21",
    "next": "^15.3.3",
    "next-seo": "^6.8.0",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "swr": "^2.3.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.29",
    "@types/react": "^18.3.12",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-next": "15.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.8.3"
  }
}
