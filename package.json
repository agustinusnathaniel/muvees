{
  "name": "muvees",
  "version": "1.3.4",
  "private": true,
  "author": "agustinusnathaniel",
  "engines": {
    "node": "^24.11.x"
  },
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "lint": "next lint",
    "turbo": "pnpm dlx turbo@2.6.3",
    "build": "next build",
    "build:turbo": "pnpm turbo build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "ultracite:check": "ultracite check --no-error-on-unmatched-pattern --error-on-warnings",
    "ultracite:fix": "ultracite fix --error-on-warnings",
    "biome:ci": "biome ci",
    "start": "next start",
    "type:check": "tsc --noEmit",
    "check:turbo": "pnpm turbo ultracite:check type:check",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.30.0",
    "@emotion/react": "^11.14.0",
    "better-all": "^0.0.1",
    "framer-motion": "^12.24.10",
    "ky": "^1.14.2",
    "lodash": "^4.17.21",
    "next": "^16.1.1",
    "next-seo": "^7.0.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@commitlint/config-conventional": "^20.3.0",
    "@commitlint/cz-commitlint": "^20.3.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "commitizen": "^4.3.1",
    "commitlint": "^20.3.0",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "knip": "^5.80.0",
    "lint-staged": "^16.2.7",
    "next-sitemap": "^4.2.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.3",
    "ultracite": "7.0.8"
  }
}
